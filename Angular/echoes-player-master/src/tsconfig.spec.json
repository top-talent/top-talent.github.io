{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "../out-tsc/spec",
    "module": "commonjs",
    "target": "es5",
    "baseUrl": "",
    "paths": {
      "~/*": [
        "/*"
      ],
      "@utils/*": [
        "app/shared/utils/*"
      ],
      "@shared/*": [
        "app/shared/*"
      ],
      "@animations/*": [
        "app/shared/animations/*"
      ],
      "@core/*": [
        "app/core/*"
      ],
      "@api/*": [
        "app/core/api/*"
      ],
      "@resolvers/*": [
        "app/core/resolvers/*"
      ],
      "@store/*": [
        "app/core/store/*"
      ],
      "@mocks/*": [
        "../tests/mocks/*"
      ],
      "@env/*": [
        "environments/*"
      ]
    },
    "types": [
      "jasmine",
      "node",
      "gapi",
      "gapi.youtube",
      "gapi.auth2",
      "youtube"
    ]
  },
  "files": [
    "test.ts"
  ],
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts"
  ]
}