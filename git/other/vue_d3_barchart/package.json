{
  "name": "vued3-barchart",
  "version": "1.0.0",
  "description": "Vue component for a Bar Chart with D3",
  "main": "main.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --no-info --config build/webpack.config.js --content-base .",
    "test": "karma start build/karma.conf.js --singleRun",
    "unit-watch": "karma start build/karma.conf.js --watch",
    "lint": "eslint --ext .js,.vue src src/; exit 0;",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.config.js",
    "deploy": "npm run build && gulp deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greven/vue3d-barchart"
  },
  "keywords": [
    "vuejs",
    "d3js",
    "chart"
  ],
  "author": "Nuno M.",
  "license": "MIT",
  "dependencies": {
    "d3": "^3.5.17",
    "font-awesome": "^4.6.3",
    "normalize.css": "^4.1.1",
    "vue": "^1.0.24"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.9.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^2.0.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^1.3.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "gulp-gh-pages": "^0.5.4",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "phantomjs-prebuilt": "^2.1.7",
    "style-loader": "^0.13.1",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^8.3.1",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
